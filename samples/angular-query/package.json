    "@angular/platform-browser": "catalog:angular",
    "@angular/router": "catalog:angular",
    "@tanstack/angular-query-experimental": "^5.90.16",
    "rxjs": "catalog:angular",
    "tslib": "catalog:",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@angular/build": "catalog:angular",
    "@angular/cli": "catalog:angular",
    "@angular/compiler-cli": "catalog:angular",
    "@faker-js/faker": "catalog:tooling",
    "@testing-library/angular": "19.0.0",
    "msw": "catalog:tooling",
    "orval": "workspace:*",
    "prettier": "catalog:",
    "typescript": "catalog:",
