{
  "name": "angular-app",
  "version": "8.4.1",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --no-watch",
    "test:snapshots": "vitest run --config vitest.snapshots.ts",
    "test:snapshots:update": "yarn test:snapshots --update",
    "lint": "ng lint",
    "generate-api": "orval",
    "clean": "rimraf .turbo dist .angular src/api",
    "nuke": "rimraf .turbo dist .angular src/api node_modules"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "catalog:angular",
    "@angular/compiler": "catalog:angular",
    "@angular/core": "catalog:angular",
    "@angular/forms": "catalog:angular",
    "@angular/platform-browser": "catalog:angular",
    "@angular/router": "catalog:angular",
    "rxjs": "catalog:angular",
    "tslib": "catalog:",
    "zod": "catalog:zod3"
  },
  "devDependencies": {
    "@angular/build": "catalog:angular",
    "@angular/cli": "catalog:angular",
    "@angular/compiler-cli": "catalog:angular",
    "@faker-js/faker": "catalog:tooling",
    "msw": "catalog:tooling",
    "orval": "workspace:*",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependenciesMeta": {
    "msw": {
      "built": true
    }
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
