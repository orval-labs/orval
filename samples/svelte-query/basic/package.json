{
  "name": "svelte-query-basic",
  "version": "8.4.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "generate-api": "svelte-kit sync && orval",
    "test:snapshots": "vitest run --config vitest.snapshots.ts",
    "test:snapshots:update": "yarn test:snapshots --update",
    "test": "yarn check"
  },
  "scriptsComments": {
    "prepare": "see https://github.com/sveltejs/kit/issues/5390#issuecomment-1176480653"
  },
  "devDependencies": {
    "@faker-js/faker": "catalog:tooling",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/kit": "^1.30.4",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "catalog:",
    "eslint-plugin-svelte3": "^4.0.0",
    "msw": "catalog:tooling",
    "orval": "workspace:*",
    "prettier": "catalog:",
    "svelte": "catalog:svelte",
    "svelte-check": "catalog:svelte",
    "tslib": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@tanstack/svelte-query": "catalog:svelte",
    "axios": "catalog:axios"
  },
  "dependenciesMeta": {
    "msw": {
      "built": true
    }
  },
  "msw": {
    "workerDirectory": "static"
  }
}
