{
  "name": "basic",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "generate": "run-p 'generate:*'",
    "orval": "node ../packages/orval/dist/bin/orval.js",
    "generate:cli": "yarn orval --input ./specifications/petstore.yaml --output ./generated/cli/petstore/endpoints.ts",
    "generate:default": "yarn orval --config ./configs/default.config.ts",
    "generate:axios": "yarn orval --config ./configs/axios.config.ts",
    "generate:angular": "yarn orval --config ./configs/angular.config.ts",
    "generate:react-query": "yarn orval --config ./configs/react-query.config.ts",
    "generate:svelte-query": "yarn orval --config ./configs/svelte-query.config.ts",
    "generate:vue-query": "yarn orval --config ./configs/vue-query.config.ts",
    "generate:swr": "yarn orval --config ./configs/swr.config.ts",
    "generate:multi-file": "yarn orval --config ./configs/multi-file.config.ts",
    "generate:zod": "yarn orval --config ./configs/zod.config.ts",
    "generate:mock": "yarn orval --config ./configs/mock.config.ts",
    "generate:fetch": "yarn orval --config ./configs/fetch.config.ts",
    "generate:mcp": "yarn orval --config ./configs/mcp.config.ts",
    "generate:mcp-mutator": "yarn orval --config ./configs/mcp-mutator.config.ts",
    "generate:hono": "yarn orval --config ./configs/hono.config.ts",
    "build": "tsc",
    "build:mcp-server": "tsc ./generated/mcp/single/*.ts ./generated/mcp/single/http-schemas/*.ts --outDir ./generated/mcp/single/dist --module commonjs --target es2020 --esModuleInterop --skipLibCheck",
    "build:mcp-server-mutator": "tsc ./generated/mcp/single-mutator/*.ts ./generated/mcp/single-mutator/http-schemas/*.ts ./mutators/mcp-client.ts --outDir ./generated/mcp/single-mutator/dist --module commonjs --target es2020 --esModuleInterop --skipLibCheck --rootDir .",
    "test:mcp-inspector": "yarn build:mcp-server && npx vitest run inspector.test.ts",
    "test:mcp-auth": "yarn build:mcp-server-mutator && npx vitest run mcp-auth.test.ts"
  },
  "author": "Victor Bury",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "orval": "link:../packages/orval/dist",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "@angular/common": "^20.1.3",
    "@angular/core": "^20.1.3",
    "@faker-js/faker": "^9.8.0",
    "@hono/zod-validator": "^0.4.2",
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@tanstack/react-query": "^5.63.0",
    "@tanstack/svelte-query": "^4.24.9",
    "@tanstack/vue-query": "^4.22.0",
    "@types/react": "18.3.2",
    "axios": "^1.8.2",
    "hono": "^4.6.16",
    "msw": "^2.0.2",
    "react": "^18.3.1",
    "swr": "^2.2.4",
    "vue": "^3.3.4",
    "zod": "^3.23.8"
  }
}
