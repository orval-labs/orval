{
  "name": "orval-tests",
  "version": "8.4.2",
  "description": "",
  "author": "Victor Bury",
  "license": "ISC",
  "private": true,
  "type": "module",
  "scripts": {
    "generate-api": "run-p 'generate:*'",
    "generate:cli": "orval --input ./specifications/petstore.yaml --output ./generated/cli/petstore/endpoints.ts",
    "generate:default": "orval --config ./configs/default.config.ts",
    "generate:axios": "orval --config ./configs/axios.config.ts",
    "generate:angular": "orval --config ./configs/angular.config.ts",
    "generate:react-query": "orval --config ./configs/react-query.config.ts",
    "generate:svelte-query": "orval --config ./configs/svelte-query.config.ts",
    "generate:vue-query": "orval --config ./configs/vue-query.config.ts",
    "generate:swr": "orval --config ./configs/swr.config.ts",
    "generate:multi-file": "orval --config ./configs/multi-file.config.ts",
    "generate:zod": "orval --config ./configs/zod.config.ts",
    "generate:mock": "orval --config ./configs/mock.config.ts",
    "generate:fetch": "orval --config ./configs/fetch.config.ts",
    "generate:mcp": "orval --config ./configs/mcp.config.ts",
    "generate:hono": "orval --config ./configs/hono.config.ts",
    "test:snapshots": "vitest run --config vitest.snapshots.ts",
    "test:snapshots:update": "yarn test:snapshots --update",
    "build": "tsc && yarn verify:mock-generated",
    "verify:mock-generated": "node ./scripts/verify-mock-generated.mjs",
    "clean": "rimraf .turbo dist generated",
    "nuke": "rimraf .turbo dist generated .yarn node_modules"
  },
  "devDependencies": {
    "@angular/common": "catalog:angular",
    "@angular/core": "catalog:angular",
    "@faker-js/faker": "catalog:tooling",
    "@hono/zod-validator": "catalog:hono",
    "@modelcontextprotocol/sdk": "catalog:mcp",
    "@tanstack/react-query": "catalog:react",
    "@tanstack/svelte-query": "catalog:svelte",
    "@tanstack/vue-query": "catalog:vue",
    "@types/react": "catalog:react",
    "axios": "catalog:axios",
    "hono": "catalog:hono",
    "msw": "catalog:tooling",
    "npm-run-all2": "catalog:",
    "orval": "workspace:*",
    "prettier": "catalog:",
    "react": "catalog:react",
    "rimraf": "catalog:",
    "rxjs": "catalog:angular",
    "svelte": "catalog:svelte",
    "swr": "catalog:swr",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "vue": "catalog:vue",
    "zod": "catalog:zod3"
  }
}
