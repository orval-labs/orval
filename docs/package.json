{
  "name": "docs",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "dev": "vite dev",
    "build": "yarn llms && vite build",
    "start": "node .output/server/index.mjs",
    "types:check": "fumadocs-mdx && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "biome check",
    "format": "biome format --write",
    "deploy": "yarn build && wrangler deploy",
    "preview": "yarn build && vite preview",
    "cf-typegen": "wrangler types",
    "llms": "node scripts/build-llms.js"
  },
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.13",
    "@tanstack/react-router-devtools": "^1.157.13",
    "@tanstack/react-router-ssr-query": "^1.157.13",
    "@tanstack/react-start": "^1.157.13",
    "dedent": "^1.7.1",
    "fumadocs-core": "16.4.7",
    "fumadocs-mdx": "14.2.6",
    "fumadocs-ui": "16.4.7",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "use-debounce": "^10.1.0",
    "vite": "^7.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@cloudflare/vite-plugin": "^1.21.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.0.5",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^6.0.4",
    "wrangler": "^4.46.0"
  }
}
