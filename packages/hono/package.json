  "dependencies": {
    "@orval/core": "workspace:*",
    "@orval/zod": "workspace:*",
    "fs-extra": "^11.3.2",
    "remeda": "^2.33.6"
  },
  "devDependencies": {
    "@hono/zod-validator": "catalog:hono",
    "@types/fs-extra": "^11.0.4",
    "eslint": "catalog:",
    "rimraf": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  }
}
