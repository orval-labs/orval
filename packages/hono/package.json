{
  "name": "@orval/hono",
  "version": "8.4.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "!dist/**/*.d.ts.map",
    "!dist/**/*.d.mts.map"
  ],
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
