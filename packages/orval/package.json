{
  "name": "orval",
  "description": "A swagger client generator for typescript",
  "version": "8.4.2",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "!dist/**/*.d.ts.map",
    "!dist/**/*.d.mts.map"
  ],
  "bin": "./src/bin/orval.ts",
  "exports": {
    ".": "./src/index.ts",
    "./bin/orval": "./src/bin/orval.ts",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "bin": {
      "orval": "./dist/bin/orval.mjs"
    },
    "exports": {
      ".": "./dist/index.mjs",
      "./bin/orval": "./dist/bin/orval.mjs",
      "./package.json": "./package.json"
    }
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "keywords": [
    "rest",
    "client",
    "swagger",
    "open-api",
    "fetch",
    "data fetching",
    "code-generation",
    "angular",
    "react",
    "react-query",
    "svelte",
    "svelte-query",
    "vue",
    "vue-query",
    "msw",
    "mock",
    "axios",
    "vue-query",
    "vue",
    "swr",
    "zod",
    "hono"
  ],
  "author": {
    "name": "Victor Bury",
    "email": "victor@anymaniax.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orval-labs/orval.git"
  },
  "scripts": {
    "build": "tsdown --config-loader unrun",
    "dev": "tsdown --config-loader unrun --watch src",
    "lint": "eslint .",
    "test": "vitest",
    "clean": "rimraf .turbo dist",
    "nuke": "rimraf .turbo dist node_modules"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@orval/angular": "workspace:*",
    "@orval/axios": "workspace:*",
    "@orval/core": "workspace:*",
    "@orval/fetch": "workspace:*",
    "@orval/hono": "workspace:*",
    "@orval/mcp": "workspace:*",
    "@orval/mock": "workspace:*",
    "@orval/query": "workspace:*",
    "@orval/solid-start": "workspace:*",
    "@orval/swr": "workspace:*",
    "@orval/zod": "workspace:*",
    "@scalar/json-magic": "^0.11.4",
    "@scalar/openapi-parser": "^0.24.13",
    "@scalar/openapi-types": "catalog:",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "commander": "^14.0.2",
    "enquirer": "^2.4.1",
    "execa": "^9.6.1",
    "find-up": "8.0.0",
    "fs-extra": "^11.3.2",
    "jiti": "^2.6.1",
    "js-yaml": "4.1.1",
    "remeda": "^2.33.6",
    "string-argv": "^0.3.2",
    "tsconfck": "^3.1.6",
    "typedoc": "^0.28.15",
    "typedoc-plugin-coverage": "^4.0.2",
    "typedoc-plugin-markdown": "^4.9.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "eslint": "catalog:",
    "rimraf": "catalog:",
    "tsdown": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "peerDependencies": {
    "prettier": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  }
}
