{
  "name": "orval",
  "description": "A swagger client generator for typescript",
  "version": "7.11.2",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": "dist/bin/orval.js",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "keywords": [
    "rest",
    "client",
    "swagger",
    "open-api",
    "fetch",
    "data fetching",
    "code-generation",
    "angular",
    "react",
    "react-query",
    "svelte",
    "svelte-query",
    "vue",
    "vue-query",
    "msw",
    "mock",
    "axios",
    "vue-query",
    "vue",
    "swr",
    "zod",
    "hono"
  ],
  "author": {
    "name": "Victor Bury",
    "email": "victor@anymaniax.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orval-labs/orval.git"
  },
  "scripts": {
    "build": "tsup ./src/bin/orval.ts ./src/index.ts --target node12 --clean --sourcemap --dts",
    "dev": "tsup ./src/bin/orval.ts ./src/index.ts --target node12 --clean --sourcemap --watch ./src --onSuccess 'yarn generate-api'",
    "lint": "eslint src/**/*.ts",
    "generate-api": "orval --config ../../samples/react-query/basic/orval.config.ts",
    "test": "tsc --noEmit && vitest --passWithNoTests"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.8",
    "@types/lodash.uniq": "^4.5.9",
    "typedoc-plugin-coverage": "^3.4.1"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@orval/angular": "workspace:*",
    "@orval/axios": "workspace:*",
    "@orval/core": "workspace:*",
    "@orval/fetch": "workspace:*",
    "@orval/hono": "workspace:*",
    "@orval/mcp": "workspace:*",
    "@orval/mock": "workspace:*",
    "@orval/query": "workspace:*",
    "@orval/swr": "workspace:*",
    "@orval/zod": "workspace:*",
    "ajv": "^8.17.1",
    "cac": "^6.7.14",
    "chalk": "^4.1.2",
    "chokidar": "^4.0.3",
    "enquirer": "^2.4.1",
    "execa": "^5.1.1",
    "find-up": "5.0.0",
    "fs-extra": "^11.3.0",
    "lodash.uniq": "^4.5.0",
    "openapi3-ts": "4.4.0",
    "string-argv": "^0.3.2",
    "tsconfck": "^2.0.1",
    "typedoc": "^0.28.7",
    "typedoc-plugin-markdown": "^4.8.0",
    "typescript": "5.8.3"
  }
}
