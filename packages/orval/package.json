{
  "name": "orval",
  "description": "A swagger client generator for typescript",
  "version": "7.11.2",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "bin": "./dist/bin/orval.js",
  "type": "commonjs",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "keywords": [
    "rest",
    "client",
    "swagger",
    "open-api",
    "fetch",
    "data fetching",
    "code-generation",
    "angular",
    "react",
    "react-query",
    "svelte",
    "svelte-query",
    "vue",
    "vue-query",
    "msw",
    "mock",
    "axios",
    "vue-query",
    "vue",
    "swr",
    "zod",
    "hono"
  ],
  "author": {
    "name": "Victor Bury",
    "email": "victor@anymaniax.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orval-labs/orval.git"
  },
  "engines": {
    "node": ">=20.19.4"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch ./src --onSuccess 'yarn generate-api'",
    "lint": "eslint src/**/*.ts",
    "generate-api": "node ./dist/bin/orval.js --config ../../samples/react-query/basic/orval.config.ts",
    "test": "tsc --noEmit && vitest"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash.uniq": "^4.5.9",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@orval/angular": "workspace:*",
    "@orval/axios": "workspace:*",
    "@orval/core": "workspace:*",
    "@orval/fetch": "workspace:*",
    "@orval/hono": "workspace:*",
    "@orval/mcp": "workspace:*",
    "@orval/mock": "workspace:*",
    "@orval/query": "workspace:*",
    "@orval/swr": "workspace:*",
    "@orval/zod": "workspace:*",
    "cac": "^6.7.14",
    "chalk": "^5.5.0",
    "chokidar": "^4.0.3",
    "enquirer": "^2.4.1",
    "execa": "^8.0.1",
    "find-up": "^7.0.0",
    "fs-extra": "^11.3.1",
    "lodash.uniq": "^4.5.0",
    "openapi3-ts": "4.5.0",
    "string-argv": "^0.3.2",
    "tsconfck": "^2.1.2",
    "typedoc": "^0.28.9",
    "typedoc-plugin-coverage": "^4.0.1",
    "typedoc-plugin-markdown": "^4.8.0"
  }
}
